        "hunt" : "~>1.2.0-rc.3"
    }, 
    "description": "D language universal object cache library. include memory、redis、memcached",
    "copyright": "Copyright (C) 2015-2019, HuntLabs",
    "homepage": "https://www.huntlabs.net",
    "license": "Apache-2.0",
    "versions": ["WITH_HUNT_CACHE"],
    "configurations":[
        {
            "name":"default",
            "dependencies": {
                    "dredis": {"path": "../dredis"}
            }
        },
        {
            "name":"normal",
            "dependencies": {
                    "dredis": {"path": "../dredis"},
                    "libmemcached":{"path": "../libmemcached"}
            }
        },
        {
            "name":"lite"
        }
    ]
}
