{
    "name": "hunt-cache",
    "dependencies": {
        "hunt" : "~>1.2.0-rc.3"
    }, 
    "description": "D language universal object cache library. include memory、redis、memcached",
    "copyright": "Copyright (C) 2015-2019, HuntLabs",
    "homepage": "https://www.huntlabs.net",
    "license": "Apache-2.0",
    "configurations":[
        {
            "name":"default",
            "dependencies": {
                    "dredis": {"version" :"~>0.0.8" }
            },
            "versions":["SUPPORT_REDIS"]
        },
        {
            "name":"normal",
            "dependencies": {
                    "dredis": {"version" :"~>0.0.8" },
                    "libmemcached":{"version":"~>1.1.1"}
            },
            "versions":["SUPPORT_REDIS","SUPPORT_MEMCACHED"]
        },
        {
            "name":"none"
        }
    ]
}
